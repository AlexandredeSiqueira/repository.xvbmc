<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon
  id="skin.kaosbox.beta"
  version="1.2.0"
  name="KAOSbox skin Jarvis"
  provider-name="KAOSbox">
  <requires>
    <import addon="xbmc.gui" version="5.10.0"/>
    <import addon="script.artistslideshow" version="1.8.0"/>
    <import addon="script.favourites" version="6.0.2"/>
  </requires>
  <extension
    point="xbmc.gui.skin"
    defaultthemename="textures.xbt"
    debugging="false"
    effectslowdown="0.00">
    <res width="1280" height="720" aspect="16:9" default="true" folder="720p" />
  </extension>
  <extension point="xbmc.addon.metadata">
   <summary lang="en">KAOSbox skin</summary>
  <description lang="en">Default skin for KAOSbox</description>
    <platform>all</platform>
  </extension>
</addon>

